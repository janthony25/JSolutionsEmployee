@model IEnumerable<Employee>

<div class="container p-4">
    <div class="row mb-4">
        <div class="col-6">
            <h2 class="text-primary">
                Employee List
            </h2>
        </div>
        <div class="col-6 text-end">
            <a class="btn btn-primary">Add New Employee</a>
        </div>
    </div>


<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th class="text-white">
                Employee Name
            </th>
            <th class="text-white">
                Position
            </th>
               
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                    <th class="text-white">
                    @obj.employeeName
                </th>
                    <th class="text-white">
                    @obj.position
                </th>
                
            </tr>
        }
    </tbody>

</table>

</div>